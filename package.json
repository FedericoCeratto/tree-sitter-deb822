{
  "name": "tree-sitter-deb822",
  "version": "0.2.1",
  "main": "bindings/node",
  "types": "bindings/node",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.22.6",
    "prebuildify": "^6.0.1"
  },
  "tree-sitter": [
    {
      "scope": "source.conf",
      "file-types": [
        "conf"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "content-regex": "(Architectures|Components|Package|Upstream-Name): "
    }
  ],
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ]
}
